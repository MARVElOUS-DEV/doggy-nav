{
  "name": "doggy-nav",
  "version": "1.0.0",
  "description": "doggy-nav",
  "main": "index.js",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "pnpm -r test",
    "server:check": "pnpm -F doggy-nav-server run static-check",
    "commit": "cz",
    "commit:help": "echo 'Use: pnpm commit or pnpm run commit to start interactive conventional commit'",
    "changelog": "changelogen --output CHANGELOG.md",
    "changelog:preview": "changelogen --dry",
    "release": "changelogen --release",
    "lint": "pnpm -r run lint",
    "lint:fix": "pnpm -r run lint:fix",
    "server:dev": "pnpm -F doggy-nav-server dev",
    "workers:dev": "pnpm -F doggy-nav-workers dev",
    "web:dev": "pnpm -F doggy-nav-main dev",
    "web:start": "pnpm -F doggy-nav-main -F doggy-nav-server start",
    "admin:dev": "pnpm -F doggy-nav-admin dev",
    "build": "pnpm -r build",
    "web:build": "pnpm -F doggy-nav-main build",
    "admin:build": "pnpm -F doggy-nav-admin build",
    "server:build": "pnpm -F doggy-nav-server build",
    "workers:build": "pnpm -F doggy-nav-workers build",
    "server:build:ci": "pnpm -F doggy-nav-server build:ci",
    "prepare": "is-ci || husky",
    "postinstall": "is-ci || husky"
  },
  "repository": "git+https://github.com/MARVElOUS-DEV/doggy-nav.git",
  "keywords": [
    "bookmarks",
    "navigation",
    "nav"
  ],
  "author": "Marvelous",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MARVElOUS-DEV/doggy-nav/issues"
  },
  "homepage": "https://github.com/MARVElOUS-DEV/doggy-nav#readme",
  "dependencies": {
    "is-ci": "^4.1.0"
  },
  "devDependencies": {
    "changelogen": "^0.6.2",
    "commitizen": "^4.3.1",
    "cross-env": "^10.1.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "packages/doggy-nav-server/app/**/*.{ts,js}": [
      "pnpm -F doggy-nav-server run lint:fix"
    ],
    "packages/doggy-nav-server/test/**/*.{ts,js}": [
      "pnpm -F doggy-nav-server run lint:fix"
    ],
    "packages/doggy-nav-server/typings/**/*.{ts,js}": [
      "pnpm -F doggy-nav-server run lint:fix"
    ],
    "packages/doggy-nav-server/adapters/**/*.{ts,js}": [
      "pnpm -F doggy-nav-server run lint:fix"
    ],
    "packages/doggy-nav-main/**/*.{ts,tsx,js,jsx}": [
      "pnpm -F doggy-nav-main run lint:fix"
    ],
    "packages/doggy-nav-admin/**/*.{ts,tsx,js,jsx}": [
      "pnpm -F doggy-nav-admin run lint:fix",
      "prettier --write"
    ],
    "**/*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.17.0"
  },
  "pnpm": {
    "overrides": {
      "@babel/runtime@<7.26.10": ">=7.26.10",
      "axios@<0.30.0": ">=0.30.0",
      "axios@<0.30.2": ">=0.30.2",
      "axios@>=0.8.1 <0.28.0": ">=0.28.0",
      "dompurify@<3.2.4": ">=3.2.4",
      "esbuild@<=0.24.2": ">=0.25.0",
      "form-data@<2.5.4": ">=2.5.4",
      "immer@>=7.0.0 <9.0.6": ">=9.0.6",
      "jsonwebtoken@<9.0.0": ">=9.0.0",
      "jsonwebtoken@<=8.5.1": ">=9.0.0",
      "koa@>=2.16.2 <2.16.3": ">=2.16.3",
      "micromatch@<4.0.8": ">=4.0.8",
      "mongoose@<6.13.5": ">=6.13.5",
      "mongoose@<6.13.6": ">=6.13.6",
      "node-fetch@<2.6.7": ">=2.6.7",
      "passport@<0.6.0": ">=0.6.0",
      "path-to-regexp@>=0.2.0 <1.9.0": ">=1.9.0",
      "postcss@<8.4.31": ">=8.4.31",
      "send@<0.19.0": ">=0.19.0",
      "tmp@<=0.2.3": ">=0.2.4",
      "tough-cookie@<4.1.3": ">=4.1.3",
      "validator@<13.15.20": ">=13.15.20",
      "vite@<4.5.10": ">=4.5.10",
      "vite@<4.5.11": ">=4.5.11",
      "vite@<4.5.12": ">=4.5.12",
      "vite@<4.5.13": ">=4.5.13",
      "vite@<=4.5.13": ">=4.5.14",
      "vite@<=4.5.5": ">=4.5.6",
      "vite@<=5.4.19": ">=5.4.20",
      "vite@>=4.0.0 <4.5.4": ">=4.5.4",
      "vite@>=4.0.0 <=4.5.2": ">=4.5.3",
      "vite@>=4.0.0 <=4.5.3": ">=4.5.4"
    }
  }
}
