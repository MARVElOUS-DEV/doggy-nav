  "bugs": {
    "url": "https://github.com/MARVElOUS-DEV/doggy-nav/issues"
  },
  "homepage": "https://github.com/MARVElOUS-DEV/doggy-nav#readme",
  "devDependencies": {
    "cross-env": "^10.0.0",
    "cz-conventional-changelog": "3.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=20.17.0"
