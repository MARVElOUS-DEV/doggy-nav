{
  "name": "doggy-nav",
  "version": "1.0.0",
  "description": "doggy-nav",
  "main": "index.js",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "pnpm -r test",
    "server:check": "pnpm -F doggy-nav-server run static-check",
    "commit": "cz",
    "commit:help": "echo 'Use: pnpm commit or pnpm run commit to start interactive conventional commit'",
    "lint": "pnpm -r run lint",
    "lint:fix": "pnpm -r run lint:fix",
    "server:dev": "pnpm -F doggy-nav-server dev",
    "workers:dev": "pnpm -F doggy-nav-workers dev",
    "web:dev": "pnpm -F doggy-nav-main dev",
    "web:start": "pnpm -F doggy-nav-main -F doggy-nav-server start",
    "admin:dev": "pnpm -F doggy-nav-admin dev",
    "build": "pnpm -r build",
    "web:build": "pnpm -F doggy-nav-main build",
    "admin:build": "pnpm -F doggy-nav-admin build",
    "server:build": "pnpm -F doggy-nav-server build",
    "server:build:ci": "pnpm -F doggy-nav-server build:ci",
    "prepare": "is-ci || husky",
    "postinstall": "is-ci || husky"
  },
  "repository": "git+https://github.com/MARVElOUS-DEV/doggy-nav.git",
  "keywords": [
    "bookmarks",
    "navigation"
  ],
  "author": "Marvelous",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MARVElOUS-DEV/doggy-nav/issues"
  },
  "homepage": "https://github.com/MARVElOUS-DEV/doggy-nav#readme",
  "devDependencies": {
    "cross-env": "^10.0.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^13.3.0",
    "prettier": "^3.6.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "packages/doggy-nav-server/app/**/*.{ts,js}": [
      "pnpm -F doggy-nav-server run lint:fix"
    ],
    "packages/doggy-nav-server/test/**/*.{ts,js}": [
      "pnpm -F doggy-nav-server run lint:fix"
    ],
    "packages/doggy-nav-server/typings/**/*.{ts,js}": [
      "pnpm -F doggy-nav-server run lint:fix"
    ],
    "packages/doggy-nav-server/adapters/**/*.{ts,js}": [
      "pnpm -F doggy-nav-server run lint:fix"
    ],
    "packages/doggy-nav-main/**/*.{ts,tsx,js,jsx}": [
      "pnpm -F doggy-nav-main run lint:fix"
    ],
    "packages/doggy-nav-admin/**/*.{ts,tsx,js,jsx}": [
      "pnpm -F doggy-nav-admin run lint:fix",
      "prettier --write"
    ],
    "**/*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.17.0"
  },
  "dependencies": {
    "is-ci": "^4.1.0"
  }
}
