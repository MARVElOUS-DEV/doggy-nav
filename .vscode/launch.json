  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "nav-server",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "localRoot": "${workspaceFolder}/packages/doggy-nav-server",
      "program": "${workspaceFolder}/packages/doggy-nav-server/node_modules/egg-bin/bin/egg-bin.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "args": ["dev" ,"--port=3002"],
      "console": "integratedTerminal"
    }
  ]
}