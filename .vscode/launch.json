{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "nav-server",
      "skipFiles": ["<node_internals>/**"],
      "localRoot": "${workspaceFolder}/packages/doggy-nav-server",
      "program": "${workspaceFolder}/packages/doggy-nav-server/node_modules/egg-bin/dist/bin/cli.js",
      "envFile": "${workspaceFolder}/packages/doggy-nav-server/.env.local",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "args": ["dev", "--port=3002"],
      "console": "integratedTerminal"
    },
    {
      "name": "Next.js: dev (launch + inspect)",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/packages/doggy-nav-main/node_modules/next/dist/bin/next",
      "args": ["dev"],
      "cwd": "${workspaceFolder}/packages/doggy-nav-main",
      "env": {
        "PORT": "3001",
        "NODE_OPTIONS": "--inspect=9229 --enable-source-maps"
      },
      "console": "integratedTerminal",
      "sourceMaps": true,
      "resolveSourceMapLocations": [
        "${workspaceFolder}/packages/doggy-nav-main/**",
        "!**/node_modules/**"
      ],
      "outFiles": ["${workspaceFolder}/packages/doggy-nav-main/.next/**/*.js"],
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Attach: Next.js dev (9229)",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "restart": true,
      "address": "localhost",
      "cwd": "${workspaceFolder}/packages/doggy-nav-main",
      "sourceMaps": true,
      "resolveSourceMapLocations": [
        "${workspaceFolder}/packages/doggy-nav-main/**",
        "!**/node_modules/**"
      ],
      "outFiles": ["${workspaceFolder}/packages/doggy-nav-main/.next/**/*.js"],
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/packages/doggy-nav-main/node_modules/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "workers: dev (wrangler)",
      "cwd": "${workspaceFolder}/packages/doggy-nav-workers",
      "runtimeExecutable": "pnpm",
      "args": ["-F", "doggy-nav-workers", "run", "dev", "--", "--inspector-port", "9230"],
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Attach: workers (9230)",
      "type": "node",
      "request": "attach",
      "port": 9230,
      "restart": true,
      "address": "localhost",
      "cwd": "${workspaceFolder}/packages/doggy-nav-workers",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
